import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

  
    public static void main(String[] args) {
      
        Scanner scanner = new Scanner(System.in);

      
        String input = scanner.nextLine();

    
        String output = input.replaceAll(" ", "");

      
        System.out.println(output);

       
        scanner.close();
    }
}
