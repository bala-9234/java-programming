import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
      
        float value = scanner.nextFloat();
        
        
        DecimalFormat sixDecimal = new DecimalFormat("0.000000");
        DecimalFormat fourDecimal = new DecimalFormat("0.0000");
        DecimalFormat twoDecimal = new DecimalFormat("0.00");

        
        System.out.println(sixDecimal.format(value));  
        System.out.println(fourDecimal.format(value)); 
        System.out.println(twoDecimal.format(value));  
        System.out.println(Math.round(value));         
    }
}
  
